<h1>People &amp; Places</h1><%- if current_user %>  <table class="table" >    <thead>      <tr>        <th>User</th>        <th>Actions</th>      </tr>    </thead>    <tbody>      <% @users.each do |user| %>        <tr>          <td><%= user.name %></td>          <td>            <%= link_to "Send Message", "#", class: "btn btn-success btn-xs start-conversation",              "data-sid" => current_user.id, "data-rip" => user.id %>            </td>          </tr>      <% end %>    </tbody>  </table>    <%= render 'map' %><% end %>